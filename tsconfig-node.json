{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es5",
    "noImplicitAny": true,
    "noImplicitReturns": false,
    "noImplicitThis": false,
    "outDir": "build",
    "inlineSourceMap": true,
    "inlineSources": true,
    "declaration": false,
    "downlevelIteration": true,
    "noEmitOnError": false,
    "stripInternal": false,
    "strict": true,
    "lib": [
      "es5",
      "dom",
      "es2015.promise",
      "es2015.symbol",
      "es2015.symbol.wellknown"
    ]
  },
  "exclude": [
    "node_modules",
    "bazel-out",
    "build",
    "build-esm",
    "build-esm-2015",
    "dist",
    "lib/closure"
  ]
}
